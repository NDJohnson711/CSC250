        int[] darn = new int[0];
        //Books book = new Books();
        //System.out.println(file);
        Scanner input = new Scanner(new File(System.getProperty("user.dir") + "/src/input.dat"));
        
        while(input.hasNext())
        {
            numLines++;
            input.nextLine();
        }
        BibleBook2[] books = new BibleBook2[numLines];
        Scanner scan = new Scanner(System.in);
        String word = scan.nextLine();
        word.Books();
        if (word != word.Books())
        {
            System.out.println("No words that match this word");
        }
        else 
        {
           System.out.println(word + BibleBook2.Books());
        }
        
        
            
        
        
